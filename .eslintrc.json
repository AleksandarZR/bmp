{
    "extends": ["next/core-web-vitals"],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "off",
        "quotes": ["warn", "double"],
        "semi": ["warn", "always"],
        "indent": ["warn", 4],
        "no-multiple-empty-lines": ["warn", { "max": 1, "maxEOF": 1, "maxBOF": 0 }]
    }
}
